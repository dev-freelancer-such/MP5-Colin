{
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": true,
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^(react|react-dom|react-(.*)|react/(.*))|antd|lodash",
    "<THIRD_PARTY_MODULES>",
    "^@/(constants|helpers/enums)",
    "^@/(config|settings|models|types)",
    "^@/(hooks|store|services)",
    "^@/(utils|services|HOCs|routes)",
    "^@/(components|containers|layouts|pages)",
    "^@/assets",
    "^[./](?!.*\\.(css|scss|sass|less)$)",
    "^[./].*\\.(css|scss|sass|less)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderGroupNamespaceSpecifiers": true,
  "importOrderCaseInsensitive": true
}
