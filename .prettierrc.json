{
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": true,
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^(react|react-dom|react/(.*))",
    "^react-(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^@/hooks/(.*)$",
    "^@/store/(.*)$",
    "^@/constants/(.*)$",
    "^@/helpers/(.*)$",
    "^@/settings/(.*)$",
    "^@/models/(.*)$",
    "^@/types/(.*)$",
    "^@/config/(.*)$",
    "^@/utils/(.*)$",
    "^@/services/(.*)$",
    "^@/HOCs/(.*)$",
    "^@/components/(.*)$",
    "^@/containers/(.*)$",
    "^@/pages/(.*)$",
    "^@/routes/(.*)$",
    "^@/layouts/(.*)$",
    "^@/assets/icons/(.*)$",
    "^@/assets/images/(.*)$",
    "^@/assets/(.*)$",
    "^[./].*\\.(?:css|scss|sass|less)$",
    "^[./]"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true,
  "importOrderGroupNamespaceSpecifiers": true,
  "importOrderCaseInsensitive": true
}
